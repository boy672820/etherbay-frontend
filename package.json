{
  "name": "etherbay-frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/theme": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@mdi/js": "^6.5.95",
    "@smui/button": "^6.0.0-beta.13",
    "@smui/card": "^6.0.0-beta.13",
    "@smui/chips": "^6.0.0-beta.13",
    "@smui/circular-progress": "^6.0.0-beta.15",
    "@smui/dialog": "^6.0.0-beta.13",
    "@smui/drawer": "^6.0.0-beta.13",
    "@smui/icon-button": "^6.0.0-beta.13",
    "@smui/layout-grid": "^6.0.0-beta.13",
    "@smui/list": "^6.0.0-beta.13",
    "@smui/menu": "^6.0.0-beta.13",
    "@smui/menu-surface": "^6.0.0-beta.13",
    "@smui/textfield": "^6.0.0-beta.15",
    "@smui/tooltip": "^6.0.0-beta.14",
    "@smui/top-app-bar": "^6.0.0-beta.13",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "smui-theme": "^5.0.0",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.26.0",
    "ethers": "^5.5.4",
    "yup": "^0.32.11"
  }
}
